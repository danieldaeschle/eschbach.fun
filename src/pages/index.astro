---
import Layout from "../layouts/Layout.astro";
import QuoteBox from "../components/QuoteBox.astro";
import GradientText from "../components/GradientText.astro";
import Person from "../components/Person.astro";
---

<Layout title="FUN at eschbach">
  <div class="h-screen flex flex-col justify-end bg-[#01032d]">
    <video autoplay muted loop class="h-full">
      <source src="ai-motion.mp4" type="video/mp4" />
    </video>
    <h1
      class="text-4xl font-bold absolute left-0 bottom-0 m-8 md:m-16 text-white"
    >
      Welcome to the&nbsp;<GradientText>AI/NLP</GradientText><span
        class="absolute w-[2px] h-[43px] bg-white animate-blink"></span> Team at
      eschbach.
    </h1>
  </div>
  <div class="bg-gray-100 p-16">
    <div>
      <center>
        <h1 class="text-5xl font-bold mb-16">
          <GradientText>Our Team</GradientText>
        </h1>
      </center>
    </div>
    <div class="flex gap-8 justify-center w-full flex-wrap">
      <Person
        name="Thomas"
        description="aka Mysterio"
        imageUrl="https://64.media.tumblr.com/15e548682209574bcb9d42fbbca3f28b/b056519f7816f0f0-64/s640x960/6f1b257ee7db2fef8758457a2720ab181d497077.jpg"
      />
      <Person
        name="DD"
        description="The only one who didn't study"
        imageUrl="https://www.wienerzeitung.at/_em_daten/_cache/image/1xSPmTI2uyg34gcuG-I4HG7l9TWmzHkaDCxGc6fQH8ImqylfyVqiP8__FGlxckUaj9Twk-7-WXdcs9VHwsmTVschHxVNhxRKInARYdxLvX_TnA_jSkn6fedA/220418-1143-948-0900-377142-duesentrieb.jpg"
      />
      <Person
        name="Christian"
        description="Team Leader"
        imageUrl="https://profile-images.xing.com/images/1accc34f5e4f0fb70dd8431af3490fbc-2/christian-e-matt.1024x1024.jpg"
      />
      <Person
        name="Robin"
        description="Hier kÃ¶nnte ihre Werbung stehen."
        imageUrl="https://www.superherodb.com/pictures2/portraits/10/050/850.jpg?v=1597793431"
      />
      <Person
        name="Anastasia"
        description="Cite me!"
        imageUrl="data:image/jpeg;base64,"
      />
      <Person name="Marco" description="" imageUrl="" />
      <Person name="Nikhila" imageUrl="" />
    </div>
  </div>
</Layout>

<script>
  const title = document.querySelector<HTMLSpanElement>("#team-name");
  const newTeamName = "FUN";

  setTimeout(() => {
    setTimeout(() => {
      newTeamName.split("").forEach((char, i) => {
        setTimeout(() => {
          title.innerText += char;
        }, i * 200);
      });
    }, title.innerText.length * 200 + 400);

    title.innerText.split("").forEach((_, i) => {
      setTimeout(() => {
        title.innerText = title.innerText.substring(
          0,
          title.innerText.length - 1
        );
      }, i * 200);
    });
  }, 2000);
</script>
